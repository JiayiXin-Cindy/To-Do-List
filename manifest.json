{
    "name": "Demo Extension",
    "version": "0.1",
    "description":"Scraper",
    "permissions":["<all_urls"],
    "browser_action": {
        "default_title": "Scraper",
        "default_popup": "popup.html"
    },

    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["contentScripts.js"],
        "run_at": "document_end"
    }],
    "manifest_version": 2
}
